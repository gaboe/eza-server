{
  "name": "eza-server",
  "version": "1.0.0",
  "main": "server.ts",
  "author": {
    "name": "Gabriel Ecegi"
  },
  "license": "MIT",
  "scripts": {
    "ts": "yarn tslint && tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "serve-debug": "nodemon dist/server.js",
    "dev": "concurrently -k -p \"[{name}]\" -n \"yellow.bold,cyan.bold,green.bold\" \"yarn ts\" \"yarn run serve-debug\""
  },
  "dependencies": {
    "dotenv": "^5.0.1",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.11.1",
    "concurrently": "^3.5.1",
    "nodemon": "^1.17.2",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  }
}