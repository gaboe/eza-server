{
  "name": "eza-server",
  "version": "1.0.0",
  "main": "server.ts",
  "author": {
    "name": "Gabriel Ecegi"
  },
  "license": "MIT",
  "scripts": {
    "ts": "yarn tslint && tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "serve-debug": "nodemon dist/server.js",
    "dev": "concurrently -k -p \"[{name}]\" -n \"yellow.bold,cyan.bold,green.bold\" \"yarn ts\" \"yarn run serve-debug\""
  },
  "dependencies": {
    "apollo-engine": "^1.0.4",
    "apollo-server-express": "^1.3.4",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "graphql": "^0.13.1",
    "mssql": "^4.1.0",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.3",
    "@types/dotenv": "^4.0.2",
    "@types/express": "^4.11.1",
    "@types/graphql": "^0.12.7",
    "@types/mssql": "^4.0.7",
    "@types/ramda": "^0.25.21",
    "concurrently": "^3.5.1",
    "nodemon": "^1.17.2",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1"
  }
}